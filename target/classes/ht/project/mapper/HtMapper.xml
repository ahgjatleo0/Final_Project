<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ht.project.mapper.HtMapper">

	<select id="login" resultType="ht.project.entity.User" 
	parameterType="ht.project.entity.User">
		select * from t_user where user_id = #{user_id} and user_pw = #{user_pw}
	</select>
	
	<select id="care" resultType="ht.project.entity.Health" 
	parameterType="ht.project.entity.User">
      select * from t_health where user_id = #{user_id}
   </select>
	
	<select id="getUserList" resultType="ht.project.entity.User" >
		select * from t_user where user_type ='N'
	</select>
	
	<select id="userCom" resultType="ht.project.entity.Community" 
	parameterType="ht.project.entity.User">
		select * from t_community where user_id = #{user_id}
	</select>
	
</mapper>